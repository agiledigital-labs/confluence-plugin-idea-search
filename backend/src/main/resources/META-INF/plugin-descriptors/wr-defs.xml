<bundles>
  <web-resource key="entrypoint-table">
    <transformation extension="js">
      <transformer key="jsI18n"/>
    </transformation>
    <context>table</context>
    <dependency>com.atlassian.plugins.atlassian-plugins-webresource-plugin:context-path</dependency>
    <resource type="download" name="bundled.table.js" location="frontend/bundled.table.js"/>
  </web-resource>
  <web-resource key="entrypoint-modal">
    <transformation extension="js">
      <transformer key="jsI18n"/>
    </transformation>
    <context>modal</context>
    <dependency>com.atlassian.plugins.atlassian-plugins-webresource-plugin:context-path</dependency>
    <resource type="download" name="bundled.modal.js" location="frontend/bundled.modal.js"/>
  </web-resource>
  <web-resource key="entrypoint-admin">
    <transformation extension="js">
      <transformer key="jsI18n"/>
    </transformation>
    <context>admin</context>
    <dependency>com.atlassian.plugins.atlassian-plugins-webresource-plugin:context-path</dependency>
    <resource type="download" name="bundled.admin.js" location="frontend/bundled.admin.js"/>
  </web-resource>
  <web-resource key="assets-DEV_PSEUDO_HASH">
    <transformation extension="js">
      <transformer key="jsI18n"/>
    </transformation>
    <transformation extension="soy">
      <transformer key="soyTransformer"/>
      <transformer key="jsI18n"/>
    </transformation>
    <transformation extension="less">
      <transformer key="lessTransformer"/>
    </transformation>
  </web-resource>
</bundles>